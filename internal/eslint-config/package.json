{
  "name": "@hai3/eslint-config",
  "version": "0.1.0-alpha.0",
  "description": "Layered ESLint configuration for HAI3 SDK architecture",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./base": "./dist/base.js",
    "./base.js": "./dist/base.js",
    "./sdk": "./dist/sdk.js",
    "./sdk.js": "./dist/sdk.js",
    "./framework": "./dist/framework.js",
    "./framework.js": "./dist/framework.js",
    "./react": "./dist/react.js",
    "./react.js": "./dist/react.js",
    "./screenset": "./dist/screenset.js",
    "./screenset.js": "./dist/screenset.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "@eslint/js": ">=9.0.0",
    "eslint": ">=9.0.0",
    "eslint-plugin-react-hooks": ">=5.0.0",
    "eslint-plugin-unused-imports": ">=4.0.0",
    "globals": ">=15.0.0",
    "typescript-eslint": ">=8.0.0"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "hai3"
  ]
}
